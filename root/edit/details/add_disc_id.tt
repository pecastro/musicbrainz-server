<table class="details add-disc-id">
  [% UNLESS edit.preview %]
  <tr>
    <th>[% l('Medium:') %]</th>
    <td colspan="2">
      [% IF edit.display_data.medium %]
        [% medium_link(edit.display_data.medium) %]
      [% ELSE %]
        on [% descriptive_link(edit.display_data.release) %]
      [% END %]
    </td>
  </tr>
  [% END %]
  <tr>
    <th>[% l('Disc ID:') %]</th>
    <td>[% link_cdtoc(edit.display_data.medium_cdtoc.cdtoc) %]</td>
  </tr>
</table>
